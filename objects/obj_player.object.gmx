            <string>if cinematic=false{
    key_right = keyboard_check(vk_right) &amp;&amp; stopped=false
    key_left = keyboard_check(vk_left) &amp;&amp; stopped=false 
    key_up = keyboard_check(vk_up) &amp;&amp; stopped=false 
    key_down = keyboard_check(vk_down) &amp;&amp; stopped=false


    //Sprite Walking Right
    if Looking = "Right" &amp;&amp; yet=false{
        sprite_index = spr_player_rw;
        image_speed = 0.2;
    }
    //Sprite Walking Left
    if Looking = "Left" &amp;&amp; yet=false{
        sprite_index = spr_player_lw;
        image_speed = 0.2;
    }
    //Sprite Walking Up
    if Looking = "Up" &amp;&amp; yet=false{
        sprite_index = spr_player_uw;
        image_speed = 0.2;
    }
    //Sprite Walking Down
    if Looking = "Down" &amp;&amp; yet=false{
        sprite_index = spr_player_dw;
        image_speed = 0.2;
    }
        
    //Sprite Stand Right
    if (yet=true &amp;&amp; Looking=="Right") 
        || (Looking="Right" &amp;&amp; key_left)
        || (Looking="Right" &amp;&amp; key_up)
        || (Looking="Right" &amp;&amp; key_down){
        sprite_index = spr_player_rs;
    }
    //Sprite Stand Left
    if (!key_left &amp;&amp; Looking=="Left") 
        || (Looking="Left" &amp;&amp; key_right)
        || (Looking="Left" &amp;&amp; key_up)
        || (Looking="Left" &amp;&amp; key_down){
        sprite_index = spr_player_ls;
    }
    //Sprite Stand Up
    if (!key_up &amp;&amp; Looking=="Up") 
        || (Looking="Up" &amp;&amp; key_left)
        || (Looking="Up" &amp;&amp; key_right)
        || (Looking="Up" &amp;&amp; key_down){
        sprite_index = spr_player_us;
    }
    //Sprite Stand Down
    if (!key_down &amp;&amp; Looking=="Down") 
        || (Looking="Down" &amp;&amp; key_left)
        || (Looking="Down" &amp;&amp; key_up)
        || (Looking="Down" &amp;&amp; key_right){
        sprite_index = spr_player_ds;
    }
    
    //If player is ready to move
    if yet=true{     
        //Move Right
        if key_right &amp;&amp; !key_up &amp;&amp; !key_down &amp;&amp; !key_left &amp;&amp; place_free(x+32,y){
            yet=false
            moveTimer=gridSize;
            xSpeed=moveSpeed;
            ySpeed=0;
            Looking = "Right"
        }
        if key_right &amp;&amp; !key_up &amp;&amp; !key_down &amp;&amp; !key_left{
            Looking = "Right"
        }
    
        //Move Left
        if key_left &amp;&amp; !key_up &amp;&amp; !key_down &amp;&amp; !key_right &amp;&amp; place_free(x-32,y){
            yet=false
            moveTimer=gridSize;
            xSpeed=-moveSpeed;
            ySpeed=0;
            Looking = "Left"
        }
        if key_left &amp;&amp; !key_up &amp;&amp; !key_down &amp;&amp; !key_right{
            Looking = "Left"
        }
    
        //Move Up
        if key_up &amp;&amp; !key_right &amp;&amp; !key_left &amp;&amp; !key_down &amp;&amp; place_free(x,y-32){
            yet=false
            moveTimer=gridSize;
            xSpeed=0;
            ySpeed=-moveSpeed;
            Looking = "Up"
        }
        if key_up &amp;&amp; !key_right &amp;&amp; !key_left &amp;&amp; !key_down {
            Looking = "Up"
        }
    
        //Move Down
        if key_down &amp;&amp; !key_right &amp;&amp; !key_left &amp;&amp; !key_up &amp;&amp; place_free(x,y+32){
            yet=false
            moveTimer=gridSize;
            xSpeed=0;
            ySpeed=moveSpeed;
            Looking = "Down"
        }
        if key_down &amp;&amp; !key_right &amp;&amp; !key_left &amp;&amp; !key_up{
            Looking = "Down"
        }
    }  
    
    if yet=false{
        x+=xSpeed;
        y+=ySpeed;
        moveTimer-=moveSpeed
        if moveTimer==0 yet=true;
    }
    
}

    

</string>
